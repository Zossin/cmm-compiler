int max(int a[2][3]) {
    int max = a[0][0], i = 0, j = 0, n = 2, m = 3;
    while (i < n) {
        while (j < m) {
            if (a[i][j] > max)
                max = a[i][j];
            j = j + 1;
        }
        i = i + 1;
        j = 0;
    }
    return max; 
}

int main() {
    int b[2][3];
    int i = 0, j = 0, n = 2, m = 3;
    while (i < n) {
