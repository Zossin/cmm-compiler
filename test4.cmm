int compare(int a, int b) {
    if (a > b)
        return 1;
    else if (a < b)
        return -1;
    else
        return 0;
}

int cal(int a, int b, int c) {
    return a * 100 + b * 10 + c;
}

int main() {
    int a = read();
    int b = read(), c = read();
    write(compare(a, b));
    write(cal(a, b, c));
    return 0;
}
